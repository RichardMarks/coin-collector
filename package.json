{
  "name": "coin-collector",
  "version": "1.0.0",
  "description": "coin collector is a small HTML5 game built using CoffeeScript where the objective is to uncover as many coins as you can before the timer reaches zero",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers coffee:coffee-script/register",
    "watch-test": "mocha --compilers coffee:coffee-script/register --watch",
    "lint": "gulp lint",
    "clobber": "gulp clobber",
    "docs": "coffeedoc -o docs src",
    "start": "gulp",
    "build": "gulp build",
    "dist": "NODE_ENV=production && gulp build",
    "clean": "gulp clean"
  },
  "keywords": [
    "CoffeeScript",
    "game",
    "HTML5"
  ],
  "author": "Richard Marks, Stephen Collins",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^13.1.1",
    "chai": "^3.5.0",
    "coffee-script": "^1.12.2",
    "coffeedoc": "^0.3.1",
    "coffeeify": "^2.1.0",
    "coffeelint": "^1.16.0",
    "gulp": "^3.9.1",
    "gulp-coffeeify": "^0.1.8",
    "gulp-coffeelint": "^0.6.0",
    "gulp-connect": "^5.0.0",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.11",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
